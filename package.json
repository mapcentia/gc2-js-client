{
  "name": "@mapcentia/gc2-js-client",
  "version": "0.0.23",
  "description": "JS client for GC2 SQL API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "build"
  ],
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "tsdown"
  },
  "devDependencies": {
    "tsdown": "^0.15.7",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "private": false
}
