{
  "name": "@mapcentia/gc2-js-client",
  "version": "0.0.23",
  "description": "JS client for GC2 SQL API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "build"
  ],
  "scripts": {
    "dev": "webpack -w",
    "build": "tsup"
  },
  "devDependencies": {
    "ts-loader": "^9.5.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "private": false
}
